pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
		echo 'Hello World'
		echo 'Hello World Times Three'}
}
	stage('Setup') {
    		sh 'su - jenkins -c "php -v"'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
        }
    }
}

